{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.1.*",
    "illuminate/html": "~5.0",
    "patricktalmadge/bootstrapper": "~5",
    "mcamara/laravel-localization": "dev-master",
    "laracasts/flash": "~1.3",
    "simplesoftwareio/simple-qrcode": "1.2.*",
    "barryvdh/laravel-dompdf": "0.5.*",
    "laracasts/utilities": "~2.0",
    "laravel/cashier": "~5.0",
    "jorgelsaud/mercado-pago": "dev-master",
    "jorgelsaud/carbonlocalizer": "dev-master"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "aws/aws-sdk-php": "~3.0",
    "iron-io/iron_mq": "~2.0",
    "mockery/mockery": "~0.9.1",
    "pda/pheanstalk": "~3.0",
    "barryvdh/laravel-debugbar": "~2.0",
    "phpunit/phpunit": "~4.0",
    "predis/predis": "~1.0",
    "phpspec/phpspec": "~2.1",
    "barryvdh/laravel-ide-helper": "~2.0",
    "doctrine/dbal": "2.6.*@dev",
    "laracasts/generators": "dev-master",
    "laracasts/testdummy": "~2.1",
    "jorgelsaud/materializer": "dev-master",
    "laracasts/integrated":"dev-master"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
