{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework","laravel"],
  "license": "MIT",
  "type": "project",
  "repositories":[
	  {
		  "type":"vcs",
		  "url":"https://github.com/giorgiosaud/bootstrapper.git"
	  }
  ],
  "require": {
    "laravel/framework": "5.1.*",
    "laravelcollective/html": "5.1.*",
    "patricktalmadge/bootstrapper": "dev-master",
    "mcamara/laravel-localization": "1.0.*",
    "laracasts/flash": "~1.3",
    "simplesoftwareio/simple-qrcode": "1.2.*",
    "barryvdh/laravel-dompdf": "0.5.*",
    "laracasts/utilities": "~2.0",
    "jorgelsaud/mercado-pago": "dev-master",
    "jorgelsaud/carbonlocalizer": "dev-master",
    "jorgelsaud/materializer": "dev-master",
    "guzzlehttp/guzzle": "~5.3|~6.0"

    },
    "require-dev": {
      "barryvdh/laravel-debugbar": "~2.0",
      "phpunit/phpunit": "~4.0",
      "phpspec/phpspec": "~2.1",
      "barryvdh/laravel-ide-helper": "~2.0",
      "doctrine/dbal": "2.6.*@dev",
      "laracasts/behat-laravel-extension": "~1.0",
      "behat/behat": "~3.0",
      "behat/mink": "~1.6",
      "behat/mink-extension": "~2.0",
      "laracasts/generators": "dev-master",
      "laracasts/testdummy": "dev-master"
      },
      "autoload": {
        "classmap": [
        "database"
        ],
        "psr-4": {
          "App\\": "app/"
        }
        },
        "autoload-dev": {
          "classmap": [
          "tests/TestCase.php"
          ]
          },
          "scripts": {
            "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
            ],
            "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
            ],
            "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
            ]
            },
            "config": {
              "preferred-install": "dist"
            }
          }
